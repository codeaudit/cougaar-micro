  <resource hfom="2.0"> org.cougaar.microedition.tini.TiniGPSLocationControllerResource</resource>

  <plugin resource="TiniGPSLocationControllerResource" command="ReportPosition"> org.cougaar.microedition.plugin.ControlPlugIn</plugin>

  <plugin buzztime="10000" weaponspeed="0.5">org.cougaar.microedition.tini.TiniBuzzerPlugIn</plugin>

  <!-- Infrastructure PlugIns -->
